{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "es6",
    "paths": {
      "@/*": [
        "src/*"
      ],        
      "@/services/*": [
        "src/services/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/views/*": [
        "src/views/*"
      ] 

    }
  },
  "include": [
    "./src/**/*"
  ]
}